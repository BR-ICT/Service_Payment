	<parameter name="QueryStringGRNMAIN" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["select h.EPPA_NO,eatx40,  ROW_NUMBER() OVER () rownumber,SUBSTR(h.EPPA_DATE,1,4) CONCAT '-' CONCAT SUBSTR(h.EPPA_DATE,5,2)  CONCAT '-' CONCAT SUBSTR(h.EPPA_DATE,7,2) as DateS,          SUBSTR(h.EPPA_DUEDT,1,4) CONCAT '-'  CONCAT SUBSTR(h.EPPA_DUEDT,5,2) CONCAT '-' CONCAT SUBSTR(h.EPPA_DUEDT,7,2) as DueDates , h.EPRA_SUNO,idsunm as SupplierName, h.EPPA_COCE,       CASE WHEN h.EPPA_PAMT=1 THEN 'Cash'  WHEN h.EPPA_PAMT=2 THEN 'Cheque No'   WHEN h.EPPA_PAMT=3 THEN  'Bank Transfer'  End as EPPA_PAMT , h.EPRA_PARM, h.EPRA_REQBY, h.EPRA_ADVREF, h.EPRA_ADVAMT  ,d.* ,  SUBSTR(d.GRNP_GRND,1,4) CONCAT '-' 	CONCAT SUBSTR(d.GRNP_GRND,5,2) CONCAT '-' CONCAT SUBSTR(d.GRNP_GRND,7,2) as GRNDATE FROM brldta0100.epr_hpayfc as h LEFT JOIN  (SELECT idsuno,idsunm	FROM m3fdbprd.cidmas	WHERE idstat='20' AND idcono='400')as sup ON sup.idsuno=h.epra_suno LEFT JOIN  (SELECT  eaaitm,eatx40	FROM m3fdbprd.fchacc	WHERE eaaitp =  2  and   eadivi ='400')as costcenter ON costcenter.eaaitm=h.EPPA_COCE LEFT JOIN  brldta0100.epr_lpayfc as d   ON h.EPPA_NO=d.GRNP_NO WHERE h.EPPA_NO='1000418'"]]></defaultValueExpression>
	</parameter>
	<parameter name="imagesDir" class="java.lang.String"/>
	<parameter name="ordernum" class="java.lang.String"/>
	<parameter name="cono" class="java.lang.String"/>
	<parameter name="divi" class="java.lang.String"/>
	<queryString>
		<![CDATA[select CCROW3,h.EPPA_NO,eatx40,  ROW_NUMBER() OVER () rownumber,SUBSTR(h.EPPA_DATE,7,2) CONCAT '/' CONCAT SUBSTR(h.EPPA_DATE,5,2) 
 CONCAT '/' CONCAT SUBSTR(h.EPPA_DATE,1,4) as DateS,          SUBSTR(h.EPPA_DUEDT,7,2) CONCAT '/' 
  CONCAT SUBSTR(h.EPPA_DUEDT,5,2) CONCAT '/' CONCAT SUBSTR(h.EPPA_DUEDT,1,4) as DueDates 
   , h.EPRA_SUNO,sasunm as SupplierName, h.EPPA_COCE,       CASE WHEN h.EPPA_PAMT=1 THEN 'Cash'
    WHEN h.EPPA_PAMT=2 THEN 'Cheque No'   WHEN h.EPPA_PAMT=3 THEN  'Bank Transfer'  End as EPPA_PAMT 
     , h.EPRA_PARM, h.EPRA_REQBY, h.EPRA_ADVREF, h.EPRA_ADVAMT 
      ,d.* ,  SUBSTR(d.GRNP_GRND,7,2) CONCAT '/' 
      CONCAT SUBSTR(d.GRNP_GRND,5,2) CONCAT '/' CONCAT SUBSTR(d.GRNP_GRND,1,4) as GRNDATE
       FROM brldta0100.PAYMENTHEAD  as h 
         LEFT JOIN  ( select SACONO,sasunm,sasuno  	FROM  m3fdbprd.cidadr
          where  saadte = '2'       and saadid = 'ENAME'        AND SACONO=$P{cono})as sup ON sup.sasuno=h.epra_suno AND sup.sacono = h.EPPA_CONO
           LEFT JOIN  (SELECT  EACONO,eadivi,eaaitm,eatx40 
FROM m3fdbprd.fchacc
 WHERE eaaitp =  2  and   eadivi =$P{divi} and  EACONO=$P{cono})as costcenter ON costcenter.eaaitm=h.EPPA_COCE AND h.EPPA_CONO = COSTCENTER.EACONO
 AND COSTCENTER.eadivi = h.EPPA_DIVI 
  LEFT JOIN  brldta0100.PAYMENTLINEGRN as d   ON h.EPPA_NO=d.GRNP_NO AND   d.GRNP_CONO = h.EPPA_CONO AND d.GRNP_DIVI = h.EPPA_DIVI
  LEFT JOIN (SELECT CCROW3,CCDIVI,CCCONO FROM M3FDBPRD.CMNDIV WHERE CCDIVI != '' ) AS z ON z.cccono = H.EPPA_CONO AND z.CCdivi = H.EPPA_DIVI 
  WHERE h.EPPA_NO=  $P{ordernum} AND h.EPPA_CONO = $P{cono} AND h.EPPA_DIVI = $P{divi} order by GRNP_GRN,grndate,GRNP_PO,(grnp_invc) ASC]]>
	</queryString>
